                                    <div class="info-row">
                                        <div>
                                            <span>Location</span>
                                            <a class="is-inverted">{{ user.localisation }}</a>
                                        </div>
                                        <i class="mdi mdi-earth"></i>
                                    </div>
                                {% elseif user.role=="Admin" %}
                                    <div class="info-row">
                                        <div>
                                            <span>departement</span>
                                            <a class="is-inverted">{{ user.departement }}</a>
                                        </div>
                                        <i class="mdi mdi-earth"></i>
                                    </div>
                                {% elseif user.role=="Etudiant" %}
                                    <div class="info-row">
                                        <div>
                                            <span>speciality</span>
                                            <a class="is-inverted">{{ user.domaine }}</a>
                                        </div>
                                        <i class="mdi mdi-earth"></i>
                                    </div>
                                    <div class="info-row">
                                        <div>
                                            <span>class</span>
                                            <a class="is-inverted">{{ user.class }}</a>
                                        </div>
                                        <i class="mdi mdi-earth"></i>
                                    </div>
                                {% elseif user.role=="Professeur" %}
                                    <div class="info-row">
                                        <div>
                                            <span>speciality</span>
                                            <a class="is-inverted">{{ user.domaine }}</a>
                                        </div>
                                        <i class="mdi mdi-earth"></i>
                                    </div>
